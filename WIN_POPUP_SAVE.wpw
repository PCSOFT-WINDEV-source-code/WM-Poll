    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         //************** Check the input
         
         // Checks whether the name is not empty
         IF EDT_PollName = "" THEN
         	Error("Specify a name")
         	SetFocus(EDT_PollName)
         	RETURN
         END
         
         // Checks whether the name is already used
         HReadSeekFirst(Poll,PollName, EDT_PollName)
         IF HFound(Poll) THEN
         	Error("This name is already used")
         	SetFocus(EDT_PollName)
         	RETURN
         END
         
         //Save the poll in the database
         HReset(Poll)
         Poll.CreatedOn	 	= Today
         Poll.CreatedBy	= EDT_Author
         Poll.PollName			= EDT_PollName
         HAdd(Poll)
         gbModifiedPoll = True
         
         MyWindow..ReturnedValue = Poll.PollID
         
         Close()
        type : 18
      -
        type : 161
        enabled : false
