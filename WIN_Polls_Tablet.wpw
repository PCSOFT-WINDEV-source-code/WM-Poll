     name : CreateChart
     procedure_id : 1760752659413632625
     type_code : 14
     code : |1+
      // Summary: Create the chart of poll results for a given answer
      // Syntax:
      // CreateChart ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE CreateChart()
      
      sAnswer is string
      
      
      grDeleteSeries(CHART_Chart,1)
      
      // Add the data
      FOR i = 1 TO csMaxNbAnswers
      	
      	sAnswer = ExtractString(ARR_ANSWERS[QUESTIONNUM_CURRENT]:sListAnswers,i,CR)
      	IF sAnswer <> EOT AND sAnswer <> "" THEN
      		grAddData(CHART_Chart,1,i,  ARR_ANSWERS[QUESTIONNUM_CURRENT]:arrAnswer[i])
      		grCategoryLabel(CHART_Chart,i,sAnswer)
      		grSeriesColor(CHART_Chart,i, HSL(7,i*20,100-i*10))
      	END
      	
      END
      
      // Draw the chart
      grDraw(CHART_Chart)
     type : 458752
   -
     name : UPDResult
     procedure_id : 1760752659413698161
     type_code : 14
     code : |1+
      // Syntax: Display the result for a given question of the poll
      //UPDResult ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procédure UPDResult()
      
      if QUESTIONNUM_CURRENT <= 0 then QUESTIONNUM_CURRENT = 1
      if QUESTIONNUM_CURRENT > ARR_ANSWERS..Occurrence then QUESTIONNUM_CURRENT = ARR_ANSWERS..Occurrence
      
      // Display the arrows
      switch QUESTIONNUM_CURRENT 
      
      	case 1 
      		BTN_Previous..Visible = False
      		BTN_Next..Visible = True
      		
      		
      	case Dimension(ARR_ANSWERS) 
      		BTN_Previous..Visible = True
      		BTN_Next..Visible = False
      		
      	other case
      		BTN_Previous..Visible = True
      		BTN_Next..Visible = True
      		
      end
      
      if ARR_ANSWERS..Empty = False then
      	
      	// Update the information
      	STC_QuestionNumber = StringBuild(<§$0002§>,QUESTIONNUM_CURRENT)
      	STC_QuestionCaption =  SPC + SPC + ARR_ANSWERS[QUESTIONNUM_CURRENT]:sQuestionCaption
      	STC_ANSWERS = ARR_ANSWERS[QUESTIONNUM_CURRENT]:sListAnswers
      	
      	// Create the chart
      	if ARR_ANSWERS[QUESTIONNUM_CURRENT]:bAnswers = True then
      		STC_NOANSWER..Visible = False
      		CHART_Chart..Visible = True
      		CreateChart()	
      	else
      		CHART_Chart..Visible = False
      		STC_NOANSWER..Visible = True
      	end
      	
      end
      
      
      
