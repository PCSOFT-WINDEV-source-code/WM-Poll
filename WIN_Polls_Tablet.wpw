      // Summary: <specify the action of the procedure>
      // Syntax:
      // New_Procedure ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE ReinitQuestionForm()
      
      //Erases everything to create a new question
      HReset(Question)
      
      //Re-initialize the controls
      EDT_QuestionCaption = ""
      RADIO_AnswerType = 1
      MZ_Answers..Visible = True
      
      mlzDeleteAll(MZ_Answers)
      mlzAddLine(MZ_Answers)
      
      EDT_MinNbAnswers = 1
      EDT_MaxNbAnswers = 1 
      
