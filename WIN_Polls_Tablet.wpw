   -
     name : CheckInputs
     procedure_id : 1760752659414025841
     type_code : 14
     code : |1-
      // Summary: Check the inputs for the addition of a new question
      // Syntax:
      //[ <Result> = ] CheckInputs ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	boolean: True if everything was ok, False otherwise
      
      PROCEDURE CheckInputs()
      
      // Check the values entered
      IF EDT_QuestionCaption = "" THEN
      	Error(<§$000a§>)
      	RESULT False
      END
      
      
      nNbAnswersEntered is int
      FOR i = 1 _TO_ MZ_Answers..Occurrence 
      	IF MZ_Answers[i].EDT_Answer <> "" THEN
      		nNbAnswersEntered++
      	END
      END 
      
      IF nNbAnswersEntered = 0 THEN
      	Error(<§$000b§>)
      	RESULT False
      END
      //Shield but should never occur because of the "whenever modified" code of the "minimum answer" and "maximum answer" edit controls
      IF EDT_MaxNbAnswers < EDT_MinNbAnswers THEN
      	Error(<§$000d§>)
      	RESULT False
      END
      
      IF nNbAnswersEntered < EDT_MinNbAnswers  THEN
      	Error(StringBuild(<§$000c§>, EDT_MinNbAnswers, nNbAnswersEntered))
      	RESULT False
      END
      
      IF EDT_MaxNbAnswers > nNbAnswersEntered THEN
      	Error(StringBuild(<§$000f§>, EDT_MaxNbAnswers, nNbAnswersEntered))
      	RESULT False
      END
      
      RESULT True
     type : 458752
   -
     name : AddModifQuestion
     procedure_id : 1760752659414091377
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      //AddModifQuestion ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE AddModifQuestion()
      
      // Manage the addition or the modification of the question
      SWITCH gnQuestionMode
      
      	CASE CST_CREATEQUESTION :
      		
      		// Update the HyperFileSQL context
      		Question.PollID = CURRENT_POLLID
      		Question.CaptionQuestions = EDT_QuestionCaption
      		Question.MinAnswer = EDT_MinNbAnswers
      		Question.MaxAnswers = EDT_MaxNbAnswers
      		Question.QuestionNum = QUESTIONNUM_CURRENT
      		Question.AnswerType = RADIO_AnswerType
      		
      		FOR i = 1 _TO_ MZ_Answers..Occurrence
      			{"Question.Answer"+i,indItem} = MZ_Answers[i].EDT_Answer
      		END
      		
      		// Adds the question
      		HAdd(Question)
      		
      		Poll.NbQuestions = Poll.NbQuestions + 1
      		HModify(Poll)
      		gbModifiedPoll = True
      		
      	CASE CST_MODIFYQUESTION :
      		
      		//Position on the question to modify
      		
      		HReadSeekFirst(Question,QuestionID, CURRENT_QUESTIONID)
      		IF HFound() = True THEN 
      			Question.CaptionQuestions = EDT_QuestionCaption
      			Question.AnswerType = RADIO_AnswerType		
      			Question.MinAnswer = EDT_MinNbAnswers
      			Question.MaxAnswers = EDT_MaxNbAnswers
      			
      			FOR i = 1 TO 5
      				{"Question.Answer"+i,indItem} = ""
      			END
      			
      			FOR i = 1 _TO_ MZ_Answers..Occurrence
      				{"Question.Answer"+i,indItem} = MZ_Answers[i].EDT_Answer
      			END
      			
      			HModify(Question)
      		END
      END
      
      QUESTIONNUM_CURRENT++
      STC_NumberCurrentQuestion = StringBuild(<§$0002§>,QUESTIONNUM_CURRENT)
      
      gnQuestionMode = CST_CREATEQUESTION
      
      ReinitQuestionForm()
      
     type : 458752
