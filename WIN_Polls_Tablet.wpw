              code : |1+
               
               IF MySelf = "" THEN
               	
               	bWasEmpty = True 
               	//Don't propose to delete this row anymore because it became empty 
                   BTN_Remove..Visible = False
               	
               ELSE
               	
               	//If the row is no longer empty while it was
               	IF bWasEmpty THEN	
               		
               		//Number of answers limited to 5 in this example
               		IF MZ_Answers..Occurrence < csMaxNbAnswers THEN			
               					
               			//Propose to delete the answer
               			BTN_Remove..Visible = True 
               			
               			//And propose to create a new possible answer
               			mlzAddLine(MZ_Answers)
               		END
               		
               		bWasEmpty = False
               	END
               END
