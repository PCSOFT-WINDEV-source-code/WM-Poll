     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      // New_Procedure ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE FillLPRQuestions()
      
      nNewHeight is int
      
      LooperDeleteAll(LOOP_Questions)
      
      // Find the questions of the poll
      FOR EACH Question WHERE PollID = CURRENT_POLLID 
      	
      	// Add to the table of all questions
      	LooperAddLine(LOOP_Questions,Question.QuestionID,Question.CaptionQuestions,Question.QuestionNum,Question.AnswerType,Question.MinAnswer, Question.MaxAnswers, 
      	Question.Answer1, Question.Answer2, Question.Answer3, Question.Answer4, Question.Answer5)
      	
      	LooperSort("ATT_QuestionNum")
      	
      END
      
      IF LOOP_Questions..Occurrence = 0 THEN
      	LOOP_Questions..Visible = False
      ELSE
      	LOOP_Questions..Visible = True 
      	
      	
      	nNewHeight = LOOP_Questions..Occurrence * LOOP_Questions..InitialHeight
      	
      	//Resize the looper according to the number of questions
      	LOOP_Questions..Height = nNewHeight
      	
      END
      
      
      
