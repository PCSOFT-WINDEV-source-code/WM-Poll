   -
     name : FillArray
     procedure_id : 1760752659413829233
     type_code : 14
     code : |1-
      // Summary: Fills the array of poll results
      // Syntax:
      // FillArray ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE FillArray()
      
      Answer is STAnswerType
      sAnswer is string
      sAnswerGiven is string
      i is int = 0
      d is int = 1
      
      // Deletes everything from the array
      ArrayDeleteAll(ARR_ANSWERS)
      
      FOR EACH Question WHERE PollID = CURRENT_POLLID
      	
      	// Fill the structure of the corresponding question
      	Answer:nQuestionNum = Question.QuestionNum 
      	Answer:sQuestionCaption = Question.CaptionQuestions
      	
      	// Fill the list of possible answers
      	Answer:sListAnswers = "- " + Question.Answer1 + CR
      	
      	FOR i = 2 TO csMaxNbAnswers
      		// Retrieve the answer
      		sAnswer = {"Question.Answer" + i,indItem}
      		// If the answer differs from ""
      		IF sAnswer <>"" THEN
      			// Add it to the list
      			Answer:sListAnswers += "- " + sAnswer + CR		
      		END
      	END
      	
      	// Update the query parameters
      	QRY_Result.nQuestionID = Question.QuestionID
      	QRY_Result.nPollID = CURRENT_POLLID
      	
      	// Run the query
      	HExecuteQuery(QRY_Result)
      	
      	sAnswer =""
      	// Read the first record found
      	HReadFirst(QRY_Result)
      	// Check the number of answers
      	IF HNbRec(QRY_Result) = 0 THEN
      		Answer:bAnswers = False
      	ELSE
      		Answer:bAnswers = True
      		
      		WHILE NOT HOut(QRY_Result)
      			
      			// The answer given is the case of a radio button
      			IF Question.MaxAnswers = 1 THEN
      				
      				// If the answer saved differs from the current one, 
      				IF sAnswer <> QRY_Result.Answer THEN	
      					i=1
      					
      					// save the new answer
      					sAnswer = QRY_Result.Answer
      					
      					// Extract the answer to check from the list of possible answers
      					sAnswerGiven = ExtractString(Answer:sListAnswers,i,CR)
      					// remove the "- "
      					sAnswerGiven = sAnswerGiven[[3 TO ]]
      					
      					// As long as the maximum number of answers is not reached
      					WHILE i<=csMaxNbAnswers
      						// If the checked answer corresponds to the current answer
      						IF sAnswer = sAnswerGiven THEN
      							// Save the result
      							Answer:arrAnswer[i]= QRY_Result.NbAnswers
      							// exit the loop
      							BREAK
      						ELSE
      							// Increment the counter
      							i++
      							// Reads the next possible answer
      							sAnswerGiven = ExtractString(Answer:sListAnswers,i,CR)
      							// Remove the "- "
      							sAnswerGiven = sAnswerGiven[[3 TO ]]
      						END
      					END	
      				END
      			ELSE
      				
      				// Case of a check box
      				FOR i = 1 _TO_ Question.MaxAnswers
      					sAnswer = ExtractString(QRY_Result.Answer,i)
      					
      					FOR d = 1 TO csMaxNbAnswers 
      						sAnswerGiven = ExtractString(Answer:sListAnswers,d,CR)	
      						IF sAnswerGiven[[3 TO]] = sAnswer THEN
      							Answer:arrAnswer[d] += QRY_Result.NbAnswers
      						END
      					END
      					
      				END
      			END
      			HReadNext(QRY_Result)
      		END
      	END
      	
      	ArrayAdd(ARR_ANSWERS,Answer)
      	
      	
      	// Reset the array to 0
      	FOR i = 1 TO csMaxNbAnswers
      		Answer:arrAnswer[i] = 0
      	END
      	
      END
      // Sort the array
      ArraySort(ARR_ANSWERS,asMember,"+nQuestionNum")
