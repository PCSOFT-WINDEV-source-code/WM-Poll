      	CASE "WIN_POPUP_OPEN" :
      		
      		CURRENT_POLLID = MyChildWindow..ReturnedValue 
      		
      		// Loads the poll
      		HReadSeekFirst(Poll, PollID, CURRENT_POLLID)
      		IF HFound(Poll) = False THEN
      			
      			MyWindow..Plane = 5
