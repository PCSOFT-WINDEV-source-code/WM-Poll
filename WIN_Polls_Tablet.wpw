     name : ChangePlane
     procedure_id : 1760752659414156913
     type_code : 14
     code : |1+
      // Summary: Modify the GUI when changing plane
      // Syntax:
      //ChangePlane (<nNewPlane> is int)
      //
      // Parameters:
      //	nNewPlane (integer): Number of the plane to display
      // Return value:
      // 	None
      
      PROCEDURE ChangePlane(nNewPlane is int)
      
      SWITCH nNewPlane
      
      	CASE 3 :
      		OPT_Cancel..Visible = True 
      		OPT_OK..Visible = True 
      		
      		OPT_Add..Visible = False 
      		
      	CASE 4 : 
      		OPT_Cancel..Visible = False 
      		OPT_OK..Visible = False 
      		
      		OPT_Add..Visible = True 
      		
      	OTHER CASE
      		OPT_Cancel..Visible = False 
      		OPT_OK..Visible = False 
      		
      		OPT_Add..Visible = False 
      END
      
      
