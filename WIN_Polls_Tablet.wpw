              code : |1+
               //Different process according to whether the maximal number of possible answers was reached or not 
               IF MZ_Answers..Occurrence = csMaxNbAnswers THEN
               	
               	//Delete the current row
               	mlzDelete(MZ_Answers,MZ_Answers)
               	
               	//Check whether there is at least one empty row, otherwise add one 
               	IF MZ_Answers[MZ_Answers..Occurrence].EDT_Answer <> "" THEN
               		mlzAddLine(MZ_Answers)
               		
               		//And make the last but one row deletable
               		MZ_Answers[MZ_Answers..Occurrence - 1].BTN_Remove..Visible = True
               	END
               	
               ELSE
               	
               	//Delete the current row
               	mlzDelete(MZ_Answers,MZ_Answers)
               
