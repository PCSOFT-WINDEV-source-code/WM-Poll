              code : |1+
               //If we are not positioned on the last row, if it is empty and if there is more than 1 row in the looper, delete
               //The purpose is to have no more than 1 empty row 
               IF MZ_Answers..Occurrence > 1  _AND_ MZ_Answers <> MZ_Answers..Occurrence _AND_ MySelf = "" THEN
               	
               	//Delete the current row
               	mlzDelete(MZ_Answers,MZ_Answers)
               	
               	IF MZ_Answers..Occurrence = 1  THEN
               		MZ_Answers[1].BTN_Remove..Visible = False
               	END
               	
