   -
     name : UpdateInterface
     procedure_id : 1760752672299390106
     type_code : 14
     code : |1+
      // Summary: This procedure is used to update the interface of the Question plane
      // Syntax:
      //UpdateInterface ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE UpdateInterface(LOCAL stCurrentQuestion is QuestionType, LOCAL nNbQuestions is int)
      
      sAnswer is string
      i is int = 1
      
      
      
      STC_QUESTIONNUM = StringBuild(<§$0004§>,stCurrentQuestion:nQuestionNum, nNbQuestions)
      
      // Update the caption of the question
      StcQuestion = stCurrentQuestion:sCaption
      
      IF stCurrentQuestion:nMinAnswers = stCurrentQuestion:nMaxAnswers THEN
      	IF stCurrentQuestion:nMaxAnswers > 1 THEN
      		// Update the caption for the maximum number of answers
      		STC_MAXANSWERS = StringBuild(<§$0005§>,stCurrentQuestion:nMaxAnswers)
      	ELSE
      		// Update the caption for the maximum number of answers
      		STC_MAXANSWERS = StringBuild(<§$0009§>,stCurrentQuestion:nMaxAnswers)
      	END
      ELSE
      	STC_MAXANSWERS = 	StringBuild(<§$0006§>,stCurrentQuestion:nMinAnswers,stCurrentQuestion:nMaxAnswers)
      END
      
      
      
      // Update the values for the options of the check box
      FOR EACH string sAnswer of stCurrentQuestion:sAnswerList SEPARATED BY CR
      	nSubscript is int = mlzAddLine(MZ_ChooseAnswers)
      	MZ_ChooseAnswers[nSubscript].STC_Answer = sAnswer
      END
      		
     type : 458752
   -
     name : SaveAnswers
     procedure_id : 1760752672299455642
     type_code : 14
     code : |1-
      // Summary: Save in memory, in an array, the answers to the poll, 
      //in order to save them all at once once all the answers to the poll have been validated
      // Syntax:
      // SaveAnswers ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE SaveAnswers()
      
      sAnswer is string
      bChosen is boolean
      
      FOR i = 1 _TO_ MZ_ChooseAnswers..Occurrence
      	bChosen = MZ_ChooseAnswers[i].IMG_Checkmark..Visible
      	
      	// Save in the array
      	TheQuestion:tSelectedAnswers[i] = bChosen
      	
      	IF bChosen = True THEN 
      		// Update the answer
      		sAnswer += [TAB] + MZ_ChooseAnswers[i].STC_Answer
      	END
      END
      
      
      
      // Update the array
      TheQuestion:sAnswer = sAnswer
      
      IF MyWindow..Name = "WIN_PollForm" THEN
      	 WIN_PollForm.garrQuestions[gnQuestionNum] = TheQuestion
      END
     type : 458752
